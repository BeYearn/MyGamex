            android:name="com.coolcloud.uac.android.qihuplug.view.LoginActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar"/>
        <activity
            android:name="com.coolcloud.uac.android.api.view.RnaDialogActivity"
            android:theme="@style/CoolcloudUacActivityStyle"/>

        <activity
            android:name="com.coolcloud.uac.android.gameassistplug.GameAssistContentActivity"
            android:launchMode="singleInstance"
            android:theme="@android:style/Theme.Translucent.NoTitleBar"/>

                    <!--coolpad支付-->
        <activity
            android:name="com.yulong.paysdk.module.main.CoolPayActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/coolpay_name"
            android:theme="@android:style/Theme.NoTitleBar"/>
        <activity
            android:name="com.yulong.paysdk.module.operational.OperationActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/coolpay_name" >
        </activity>
        <activity
            android:name="com.yulong.paysdk.module.me.MeActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/coolpay_name" >
        </activity>
        <activity
            android:name="com.alipay.sdk.app.H5PayActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind"
            android:windowSoftInputMode="adjustResize|stateHidden" >
        </activity>
        <activity
            android:name="com.yulong.paysdk.module.payment.PaymentActivity"
            android:label="@string/coolpay_name"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>
        <activity
            android:name="com.yulong.paysdk.module.main.CoolPayActivityDialog"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/coolpay_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>
        <activity
            android:name="com.yulong.paysdk.module.charge.ChargeActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/coolpay_name"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.yulong.paysdk.module.qq.CallbackActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/coolpay_name"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />

                <data android:scheme="qwallet1105429247" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.unionpay.uppay.PayActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:label="@string/coolpay_name"
            android:screenOrientation="portrait" />

                <!-- alipay sdk begin -->
        <service
            android:name="com.yulong.paysdk.aidl.ExcuteFlatService"
            android:process=":remote" >
            <intent-filter>
                <action android:name="com.yulong.paysdk.aidl.ExcuteFlatService" >
                </action>
            </intent-filter>
        </service>

        <receiver android:name="com.yulong.paysdk.module.main.CoolPayActivity$ResultReceiver" >
            <intent-filter>
                <action android:name="com.yulong.paysdk.receiver.ResultReceiver.onFlatStart" />
                <action android:name="com.yulong.paysdk.receiver.ResultReceiver.onPayResult" />
            </intent-filter>
        </receiver>
        <!--coolpad 声明end-->


        <!-- 个推SDK配置开始 -->
        <meta-data
            android:name="PUSH_APPID"
            android:value="augb87iFQ1AcrdvCWfN9L4" /> <!-- 替换为第三方应用的APPID -->
        <meta-data
            android:name="PUSH_APPKEY"
            android:value="xjqZuyepvi96lbSG3JqLi7" /> <!-- 替换为第三方应用的APPKEY -->
        <meta-data
            android:name="PUSH_APPSECRET"
            android:value="JWupfp2Z9f6XBp5rqzaga3" /> <!-- 替换为第三方应用的APPSECRET -->
        <receiver
            android:name="com.emagroup.sdk.PushDemoReceiver"
            android:exported="false">
            <intent-filter>
                <action android:name="com.igexin.sdk.action.augb87iFQ1AcrdvCWfN9L4" /> <!--上面获得的PUSH_APPID-->
            </intent-filter>
        </receiver>

        <service
            android:name="com.igexin.sdk.PushService"
            android:exported="true"
            android:label="NotificationCenter"
            android:process=":pushservice">
            <intent-filter>
                <action android:name="com.igexin.sdk.action.service.message" />
            </intent-filter>
        </service>
        <service
            android:name="com.igexin.sdk.PushServiceUser"
            android:exported="true"
            android:label="NotificationCenterUser">
            <intent-filter>
                <action android:name="com.igexin.sdk.action.user.message" />
            </intent-filter>
        </service>

        <receiver android:name="com.igexin.sdk.PushReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="com.igexin.sdk.action.refreshls" />
                <action android:name="android.intent.action.MEDIA_MOUNTED" />
                <action android:name="android.intent.action.ACTION_POWER_CONNECTED" />
                <action android:name="android.intent.action.ACTION_POWER_DISCONNECTED" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.igexin.sdk.PushManagerReceiver"
            android:exported="false">
            <intent-filter>
                <action android:name="com.igexin.sdk.action.pushmanager" />
            </intent-filter>
        </receiver>

        <activity
            android:name="com.igexin.sdk.PushActivity"
            android:excludeFromRecents="true"
            android:exported="false"
            android:process=":pushservice"
            android:taskAffinity="com.igexin.sdk.PushActivityTask"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.igexin.sdk.GActivity"
            android:excludeFromRecents="true"
            android:exported="true"
            android:process=":pushservice"
            android:taskAffinity="com.igexin.sdk.PushActivityTask"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />

        <service
            android:name="com.igexin.download.DownloadService"
            android:process=":pushservice" />
        <receiver android:name="com.igexin.download.DownloadReceiver">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>
        <provider
            android:name="com.igexin.download.DownloadProvider"
            android:authorities="downloads.com.example.myggame"
            android:exported="true"
            android:process=":pushservice" /><!--"authorities' downloads后配为你的包名-->
        <!-- 个推SDK配置结束 -->


    </application>

</manifest>
